<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Analyze with SonarLint">
        <Item>
          <IdeaActionID>SonarLint.AnalyzeFiles</IdeaActionID>
          <ShortCut>'Ctrl+Shift+S'</ShortCut>
          <Description>Analyze with SonarLint</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Refresh">
        <Item>
          <IdeaActionID>FileChooser.Refresh</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Y'</ShortCut>
          <Description>Refresh</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diagram Popup...">
        <Item>
          <IdeaActionID>ShowUmlDiagramPopup</IdeaActionID>
          <ShortCut>'Ctrl+Alt+U'</ShortCut>
          <Description>Show Diagram Popup...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diagram...">
        <Item>
          <IdeaActionID>ShowUmlDiagram</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+U'</ShortCut>
          <Description>Show Diagram...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Structure">
        <Item>
          <IdeaActionID>ActivateStructureToolWindow</IdeaActionID>
          <ShortCut>'Alt+7'</ShortCut>
          <Description>Structure</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>TODO</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>